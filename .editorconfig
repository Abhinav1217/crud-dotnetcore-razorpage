root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
tab_width = 4
indent_size = 4
indent_style = tab
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

# Ignore paths
[/wwwroot/lib/**,]
charset = unset
end_of_line = unset
insert_final_newline = unset
trim_trailing_whitespace = unset
indent_style = unset
indent_size = unset

